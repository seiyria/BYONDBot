{
  "name": "byondbot",
  "version": "1.0.0",
  "description": "A bot for the BYOND discord server.",
  "main": "index.js",
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "start": "ts-node src/index",
    "start:dev": "nodemon --exec \"ts-node \"-- \"src/index.ts\"",
    "test:lint": "tslint --project .",
    "test:unit": "ava-ts src/test/**.test.ts",
    "test": "npm run test:lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BYOND/DiscordBot.git"
  },
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BYOND/DiscordBot/issues"
  },
  "homepage": "https://github.com/BYOND/DiscordBot#readme",
  "dependencies": {
    "axios": "0.19.2",
    "cheerio": "1.0.0-rc.3",
    "discord.js": "^11.5.1",
    "dotenv": "^8.0.0",
    "fuzzyset-obj": "1.0.0",
    "turndown": "6.0.0",
    "typeorm": "^0.2.25",
    "typescript-ioc": "^1.2.5"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "ava-ts": "^0.25.2",
    "nodemon": "2.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  }
